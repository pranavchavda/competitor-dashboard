{
  "name": "competitor-dash",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm run server:dev\" \"vite\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server:dev": "tsx --watch server/index.ts",
    "server:build": "tsc server/index.ts --outDir dist-server",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "pkg:build": "pnpm run build && pkg real-standalone.cjs --targets node20-win-x64,node20-macos-x64,node20-linux-x64 --out-path dist-pkg",
    "pkg:build-win": "pnpm run build && pkg real-standalone.cjs --targets node20-win-x64 --out-path dist-pkg",
    "pkg:build-mac": "pnpm run build && pkg real-standalone.cjs --targets node20-macos-x64 --out-path dist-pkg",
    "pkg:build-linux": "pnpm run build && pkg real-standalone.cjs --targets node20-linux-x64 --out-path dist-pkg",
    "build:windows-portable": "node build-windows-portable.js",
    "build:all": "pnpm run pkg:build-mac && pnpm run pkg:build-linux && pnpm run build:windows-portable"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.1.3",
    "@prisma/client": "^6.12.0",
    "@types/js-cookie": "^3.0.6",
    "@types/ws": "^8.18.1",
    "axios": "^1.7.0",
    "cheerio": "^1.0.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "esbuild": "^0.25.8",
    "express": "^4.18.2",
    "framer-motion": "^11.2.6",
    "js-cookie": "^3.0.5",
    "openai": "^5.10.2",
    "prisma": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.0.2",
    "sqlite3": "^5.1.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "pkg": {
    "assets": [
      "dist/**/*",
      "server/**/*",
      "node_modules/@prisma/client/**/*",
      "prisma/schema.prisma",
      ".env"
    ],
    "targets": [
      "node20-win-x64",
      "node20-macos-x64",
      "node20-linux-x64"
    ],
    "outputPath": "dist-pkg"
  }
}